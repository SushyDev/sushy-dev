---
type Icon = {
	image: string;
	title: string;
}

interface Props extends Icon {}

const { image, title } = Astro.props
---

<li class="flex items-center min-w-max h-12 last-of-type:pr-4">
	<img class="h-12" width="48px" height="48px" src={image} alt={title} />
	<p class="ml-2 cursor-text">{title}</p>
</li>
